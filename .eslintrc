{
    "env": {
        "node": true
    } ,
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ], "plugins": [
        "prettier"
    ],
    "root": true,
    "rules": {
        "no-console": "off",
        "prettier/prettier": [
            "error",
            {
              "endOfLine": "auto",
            },
          ],
        "no-param-reassign": [
            "error",
            {
                "props": false
            }
        ]

    }
}